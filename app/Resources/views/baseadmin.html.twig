		<script src="{{ asset('js/jquery.fancybox.js') }}"></script>


	
    </head>
    <body>
	<div class="container">
	{% block body %}{% endblock %}
	</div>
        
        {% block javascripts %}{% endblock %}
		
		<script>
			$(document).ready(function() {
				$('.photo').fancybox();
			})
		</script>

    </body>
</html>
